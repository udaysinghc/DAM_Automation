<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">

    <parameter name="Browser" value="Chrome-Windows"/>
    <parameter name="Headless" value="false"/>

    <parameter name="EnvironmentName" value="vptest"/>
    <parameter name="BaseURL" value="https://vptest.intelligencebank.com"/>
    <parameter name="SiteName" value="vptest.intelligencebank.com"/>

    <listeners>
        <listener class-name="com.intelligencebank.platform.Listeners"/>
        <listener class-name="com.intelligencebank.utils.ExtentReporterNG"/>
    </listeners>

    <test name="DPC Tests">
        <classes>
            <class name="com.intelligencebank.cucumberOptions.DCPTestRunner"/>
        </classes>
    </test>

</suite>